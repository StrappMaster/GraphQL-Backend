    "@types/lodash": "^4.14.170",
    "@types/mongoose": "^5.10.4",
    "apollo-server": "^2.19.2",
    "apollo-server-express": "^2.19.2",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "date-format": "^3.0.0",
    "dateformat": "^5.0.2",
    "debug": "~2.6.9",
    "express": "^4.16.4",
