  "engines": {
    "node": "15.0.0"
  },
  "scripts": {

    "lint": "eslint src/**/*.ts",
    "start": "ts-node ./src/server.ts",
    "dev": "nodemon --watch ./**/*.ts --exec 'npm run lint -- --fix && ts-node ./src/server.ts'",
    "dev-nolint": "nodemon --watch ./**/*.ts --exec 'ts-node ./src/server.ts'",
    "killall": "killall node",
    "gen-types": "ts-node ./src/gen_schema.ts",
    "graphql:generate": "rm -f ./src/common/schema/schema.graphql && npx @graphql-codegen/cli"
  },
  "dependencies": {
    "@graphql-codegen/cli": "^1.21.5",
